{
  "name": "furycode",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:pages": "set CF_PAGES=true && next build && node scripts/copy-pages-output.js",
    "build:cloudflare": "set CF_WORKERS=true && set CF_PAGES=false && set NODE_ENV=production && next build",
    "build:railway": "set RAILWAY=true && next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "node scripts/clean-cache.js",
    "clean:build": "if exist .next rmdir /s /q .next && if exist .vercel rmdir /s /q .vercel && if exist cache rmdir /s /q cache",
    "deploy": "wrangler deploy",
    "deploy:dev": "wrangler deploy --env=development",
    "deploy:pages": "wrangler pages deploy out --config wrangler.pages.toml",
    "deploy:pages:dev": "wrangler pages deploy out --config wrangler.pages.toml --project-name=kemono-shares-api-dev",
    "test:r2-config": "node scripts/test-r2-config.js",
    "test:upload": "node scripts/test-upload.js",
    "test:share-r2": "node scripts/test-share-r2-integration.js",
    "test:afterimage-config": "node scripts/test-afterimage-config.js",
    "test:kv-storage": "node scripts/test-kv-storage.js",
    "check:production": "node scripts/check-production-config.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.859.0",
    "@cloudflare/next-on-pages": "^1.13.13",
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/postcss": "^4.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "axios": "^1.10.0",
    "dotenv": "^17.2.0",
    "eslint": "^9",
    "next": "15.3.5",
    "next-intl": "^4.3.4",
    "postcss": "^8.5.3",
    "react": "^19.0.0",
    "react-compare-image": "^3.5.6",
    "react-dom": "^19.0.0",
    "react-intersection-observer": "^9.16.0",
    "server-only": "^0.0.1",
    "tailwindcss": "^4.0.0",
    "typescript": "^5",
    "yet-another-react-lightbox": "^3.24.0",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/webpack": "^5.28.5",
    "eslint-config-next": "15.3.5",
    "webpack-bundle-analyzer": "^4.10.2",
    "wrangler": "^4.27.0"
  }
}
