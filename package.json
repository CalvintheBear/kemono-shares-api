{
  "name": "furycode",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:railway": "next build",
    "build:cloudflare": "next build --no-lint",
    "build:pages": "node scripts/build-pages.js",
    "build:ultra": "node scripts/build-cloudflare-ultra-aggressive.js",
    "build:optimized": "node scripts/build-pages.js",
    "clean": "node scripts/clean-cache.js",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "node scripts/build-pages.js",
    "pages:deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static",
    "pages:dev": "npx @cloudflare/next-on-pages --dev",
    "deploy": "wrangler deploy --env=\"\"",
    "deploy:dev": "wrangler deploy --env=development",
    "deploy:pages": "npm run build:pages && wrangler pages deploy .vercel/output/static",
    "deploy:pages:dev": "npm run build:pages && wrangler pages deploy .vercel/output/static --project-name=kemono-shares-api-dev",
    "test:r2-config": "node scripts/test-r2-config.js",
    "test:upload": "node scripts/test-upload.js",
    "test:share-r2": "node scripts/test-share-r2-integration.js",
    "test:afterimage-config": "node scripts/test-afterimage-config.js",
    "test:optimizations": "node scripts/test-optimizations.js",
    "test:duplicate-prevention": "node scripts/test-duplicate-prevention.js",
    "test:kv-storage": "node scripts/test-kv-storage.js",
    "check:production": "node scripts/check-production-config.js",
    "clean:cache": "node scripts/clean-cache.js",
    "clean:build": "if exist .next rmdir /s /q .next && if exist .vercel rmdir /s /q .vercel && if exist cache rmdir /s /q cache",
    "fix:production": "node scripts/fix-production-errors.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.859.0",
    "@cloudflare/next-on-pages": "^1.13.13",
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/postcss": "^4.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "axios": "^1.10.0",
    "dotenv": "^17.2.0",
    "eslint": "^9",
    "next": "15.3.5",
    "next-intl": "^4.3.4",
    "postcss": "^8.5.3",
    "react": "^19.0.0",
    "react-compare-image": "^3.5.6",
    "react-dom": "^19.0.0",
    "react-intersection-observer": "^9.16.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5",
    "yet-another-react-lightbox": "^3.24.0",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/webpack": "^5.28.5",
    "eslint-config-next": "15.3.5",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
